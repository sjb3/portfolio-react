{
  "name": "portfolio-react",
  "version": "1.0.0",
  "description": "portfolio-react",
  "main": "index.js",
  "scripts": {
    "watch": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot",
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --history-api-fallback",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Justin",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-stage-2": "^6.11.0",
    "clean-webpack-plugin": "^0.1.14",
    "extract-text-webpack-plugin": "^0.3.8",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-fontawesome": "^1.4.0",
    "react-router": "^3.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.10.1",
    "eslint-plugin-react": "^6.7.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1"
  }
}
